{
    "name": "test-express-node-js",
    "title": "Example of an HTTPS Express applet",
    "summary": "httpsapp-express-js",
    "dxapi": "1.0.0",
    "version": "0.0.3",
    "inputSpec": [
      {
        "name": "bam_files",
        "label": "Bam files",
        "help": "A set of bam files.",
        "class": "array:file",
        "optional": true
      }
    ],
    "outputSpec": [],
    "httpsApp": {
        "ports": [443],
        "shared_access": "VIEW"
    },
    "timeoutPolicy": {
      "*": {
        "hours": 1
      }
    },
    "runSpec": {
      "interpreter": "bash",
      "file": "src/docker-web-app.sh",
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_x4"
        }
      },
      "distribution": "Ubuntu",
      "release": "20.04",
      "version": "0"
    },
    "access": {
      "network": [
        "*"
      ]
    }
  }